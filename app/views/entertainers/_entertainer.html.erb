<%= form_for @entertainer do |f| %>
  <%= f.label 'Username: ' %>
  <%= f.text_field :username %><br>
  <%= f.label 'Job Type: ' %>
  <%= f.collection_select :job_type, Entertainer.job_types, :to_s, :to_s %><br>
  <%= f.label 'Image: ' %>
  <%= f.text_field :image %><br>
  <%= f.label 'Rate: ' %>
  <%= f.text_field :rate %><br>
  <%= f.label 'Description of Services: ' %>
  <%= f.text_area :description %><br>
  <%= f.submit %>
<% end %>
